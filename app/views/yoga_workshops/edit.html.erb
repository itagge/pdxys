<h1>Editing yoga_workshop</h1>

<% form_for(@yoga_workshop) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :studio_id %><br />
    <%= f.text_field :studio_id %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :end_date %><br />
    <%= f.datetime_select :end_date %>
  </p>
  <p>
    <%= f.label :start_date %><br />
    <%= f.datetime_select :start_date %>
  </p>
  <p>
    <%= f.label :level %><br />
    <%= f.text_field :level %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>